package driveSchool;

import java.sql.Date;

public class User {
	private String user;
	private String password;
	private Date data;
	
	
	User(String user, String password, Date data)
	{
		this.user = user;
		this.password = password;
		this.data = data;
	}
	
	public void SetUser(String user, String password, Date data) 
	{
		this.user = user;
		this.password = password;
		this.data = data;
	}

}
