/**
 * 
 */
package dxBallpkg;

/**
 * @author valentin
 *
 */
public class navBar extends position {
	
	private int x;
	private int y;
	private int width;
	private int height;
	
	navBar(){
		width=50*7;
		height=30;
		x=5;
		y=20;
	}
	public void setBar( int x, int y, int width, int height )
	{     navBar n = new navBar();
	       n.set_position(x, y);;  
		this.x = n.x()
		this.y = n.y();
		this.width = width;
		this.height = height;
	}
	public int x()
	{
		return x;
	}
	
	public int y() {
		return y;
	}
	
	public int width() {
		return width;
	}
	
	public int height() {
		return height;
	}
	
	protected void finalize() {
		System.out.println("object is destroyed");
	}
	
	
	

}














